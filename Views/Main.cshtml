@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
    Layout = null;
    var currentCulture = Thread.CurrentThread.CurrentCulture;
    var language = currentCulture.Name;
}
<!DOCTYPE html>
<html lang="@language">
    <head>
        @await Html.PartialAsync("metadata")
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap" rel="stylesheet">
        <link rel="stylesheet" href="https://cdn.linearicons.com/free/1.0.0/icon-font.min.css">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
        <link href="/composants/style.css" rel="stylesheet" asp-append-version="true" />
        <link href="/globals.css" rel="stylesheet" asp-append-version="true" />
    </head>
    <body id="page-top">

        <partial name="mainNavigation"/>
        <partial name="filAriane"/>

        @RenderBody()

        <partial name="footer" model="Model" />

        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
        <script src="/composants/qc-components.js" asp-append-version="true"></script>

    </body>
</html>
